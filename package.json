{
  "name": "Natours",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:sass": "node-sass ./starter/sass/main.scss css/style.css -w",
    "dev": "cd ./starter && live-server",
    "start": "npm-run-all --parallel dev watch:sass",
    "compile:sass": "node-sass ./starter/sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css ./starter/css/icon-font.css ./starter/css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.concat.css -o ./starter/css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css ./starter/css/style.css --output-style compressed",
    "build": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "concat": "^1.0.3",
    "node-sass": "^6.0.0",
    "postcss-cli": "^8.3.1"
  }
}
